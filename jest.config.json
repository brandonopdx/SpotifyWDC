{
    "verbose": true,
    "testRegex": "(/src/.*|(\\.|/))test\\.(unit|integration)\\.js?$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*integration.remote.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@jaxolotl)"
    ],
    "coveragePathIgnorePatterns": [
      "/assets/",
      "/node_modules/",
      "/.git/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }